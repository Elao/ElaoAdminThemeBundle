{% use "@ElaoAdminTheme/layout/menu_item.html.twig" %}

{% block nav_tertiary %}
    <nav class="tertiary-nav">
        <ul>
            {% set item_class = 'tertiary-nav__item' %}
            {% set item_active_class = 'tertiary-nav__item--active' %}
            {% for item in menu|filter(item => item is accessible) %}
                {{ block('menu_item') }}
            {% endfor %}
        </ul>
    </nav>
{% endblock %}
